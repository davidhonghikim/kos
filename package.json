{
  "name": "kos-knowledge-library-framework",
  "version": "1.0.0",
  "description": "kOS Knowledge Library Framework - Comprehensive knowledge management and agent orchestration system",
  "main": "src/index.ts",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "vite",
    "dev:backend": "python -m uvicorn src.backend.main:app --reload --port 8000",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "vite build",
    "build:backend": "python -m build",
    "test": "npm run test:frontend && npm run test:backend",
    "test:frontend": "jest --testPathPattern=src --coverage",
    "test:backend": "pytest tests/ --cov=src --cov-report=html",
    "test:watch": "jest --watch",
    "lint": "eslint src/ --ext .ts,.tsx",
    "lint:fix": "eslint src/ --ext .ts,.tsx --fix",
    "format": "prettier --write src/",
    "type-check": "tsc --noEmit",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "setup": "npm install && pip install -r requirements.txt"
  },
  "keywords": [
    "knowledge-management",
    "agent-orchestration",
    "klf",
    "typescript",
    "python",
    "fastapi",
    "react",
    "microservices"
  ],
  "author": "kOS Development Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kos-team/kos-framework.git"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "axios": "^1.6.2",
    "socket.io-client": "^4.7.4",
    "uuid": "^9.0.1",
    "date-fns": "^2.30.0",
    "clsx": "^2.0.0",
    "tailwindcss": "^3.3.6",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32"
  },
  "devDependencies": {
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/node": "^20.10.5",
    "@types/uuid": "^9.0.7",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "@vitejs/plugin-react": "^4.2.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "@testing-library/react": "^14.1.2",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/user-event": "^14.5.1",
    "jest-environment-jsdom": "^29.7.0",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.1.1",
    "concurrently": "^8.2.2"
  }
} 